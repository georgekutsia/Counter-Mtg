{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "target": "ES2020",
    "moduleResolution": "Bundler",
    "types": ["react", "react-native"],
    "skipLibCheck": true
  }
}
